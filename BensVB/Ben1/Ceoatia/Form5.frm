Dim CurrentColumn As Integer, SpaceValue(0 To 6, 0 To 12)

Private Sub imgSpace_Click(Index As Integer)
If CurrentColumn = -1 Then
Select Case Int(Index / 13)
    Case Is < 1
    CurrentColumn = 0
    Case Is < 2
    CurrentColumn = 1
    Case Is < 3
    CurrentColumn = 2
    Case Is < 4
    CurrentColumn = 3
    Case Is < 5
    CurrentColumn = 4
    Case Is < 6
    CurrentColumn = 5
    Case Is < 7
    CurrentColumn = 7
End Select
Else
Select Case Int(Index / 13)
    Case Is < 1
    MoveTile (0)
    Case Is < 2
    MoveTile (1)
    Case Is < 3
    MoveTile (2)
    Case Is < 4
    MoveTile (3)
    Case Is < 5
    MoveTile (4)
    Case Is < 6
    MoveTile (5)
    Case Is < 7
    MoveTile (6)
End Select
End Sub

Public Sub MoveTile(DestColumn As Integer)
Dim i, movevalue As Integer
For i = 0 To 12
If SpaceValue(CurrentColumn, i) > 0 Then
movevalue = SpaceValue(CurrentColumn, i)

End Sub
