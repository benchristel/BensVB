Attribute VB_Name = "Form2"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit
Dim coded
Dim Power

Private Sub cmdFinish_Click()
Dim i
Power = ""
lblDisplay.Caption = "You have attempted to locate a despot named " & coded & ".  The selected despot could not be located."
    For i = 0 To 25
        Select Case cmdSelector(i).BackColor
        Case Is = RGB(255, 0, 0)
        Power = Power & "1"
        Case Is = RGB(100, 100, 255)
        Power = Power & "2"
        Case Else
        Power = Power & "0"
        End Select
    Next i
    Select Case Power
    Case Is = "12202202102001100000000000"
    If LCase(coded) = "ceokanio" Then
    lblDisplay.Caption = "Ceokanio Disituem.  Ruler of Ceoatia.  Born Pixi 7, 21 PTE.  Now Eating at 156,1224"
    Else
    lblDisplay.Caption = "You have entered an incorrect name, or the despotic keypad was not activated in the proper order."
    End If
    Case Else
    lblDisplay.Caption = "You have entered an incorrect name, or the despotic keypad was not activated in the proper order."
    End Select
End Sub

Private Sub cmdSelector_Click(Index As Integer)
cmdSelector(Index).BackColor = RGB(255, 0, 0)
If Index Mod 2 = 0 And Index > 0 Then
cmdSelector(Index / 2).BackColor = RGB(100, 100, 255)
Else
If Index > 2 Then cmdSelector(Index - 3).BackColor = RGB(100, 100, 255)
End If
End Sub

Private Sub Form_Load()

End Sub

Private Sub TxtEnter_Change()
Dim i
coded = ""
If Len(txtEnter.Text) > 0 Then
For i = 1 To Len(txtEnter.Text) Step 5
coded = coded & Mid(txtEnter.Text, i, 1)
Next i
End If
If Len(txtEnter.Text) > 1 Then
For i = 2 To Len(txtEnter.Text) Step 5
coded = coded & Mid(txtEnter.Text, i, 1)
Next i
End If
If Len(txtEnter.Text) > 2 Then
For i = 3 To Len(txtEnter.Text) Step 5
coded = coded & Mid(txtEnter.Text, i, 1)
Next i
End If
If Len(txtEnter.Text) > 3 Then
For i = 4 To Len(txtEnter.Text) Step 5
coded = coded & Mid(txtEnter.Text, i, 1)
Next i
End If
If Len(txtEnter.Text) > 4 Then
For i = 5 To Len(txtEnter.Text) Step 5
coded = coded & Mid(txtEnter.Text, i, 1)
Next i
End If
End Sub
